class Legacy<%= file_name.classify %> < LegacyBase
  set_table_name "<%= file_name.pluralize %>"
  # To use autogenerated ids uncomment below
  #@dont_migrate_ids = true

  def map
    {
      :field_one => self.old_field_one.squish,
      :field_two => self.old_field_two.squish
    }
  end

  def associate
    {
      :association => records.to.associate
    }
  end

end
